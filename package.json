{
  "name": "@rheactor/mariadb-rev",
  "description": "A revolution mariadb driver (Typescript-based).",
  "version": "0.1.0",
  "license": "Apache License 2.0",
  "scripts": {
    "dev": "tsc --watch",
    "build": "rimraf dist/ && tsc -project tsconfig.build.json && tsc-alias && swc ./dist -d ./dist",
    "ts:unused": "ts-unused-exports ./tsconfig.json --showLineNumber",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "test": "jest",
    "test:build": "npm run build && jest --config ./jest.config.build.ts",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --watchAll"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.38",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.3.1",
    "ts-node": "^10.9.1",
    "ts-unused-exports": "^9.0.4",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5"
  }
}
